   "sizes": "512x512",
   "type": "image/png",
   "purpose": "any"
  },
  {
   "src": "/icons/icon-192x192-maskable.png",
   "sizes": "192x192",
   "type": "image/png",
   "purpose": "maskable"
  },
  {
   "src": "/icons/apple-touch-icon.png",
   "sizes": "180x180",
   "type": "image/png",
   "purpose": "any apple"
  }
 ],
