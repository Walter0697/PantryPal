{
  "name": "stock-recorder",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.6.7"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "bash build-pages.sh",
    "pages:build:win": "powershell -File build-pages.ps1",
    "pages:deploy": "bash deploy-with-r2.sh",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "fix:large-files": "bash fix-large-files.sh",
    "fix:large-files:win": "powershell -File fix-large-files.ps1"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.760.0",
    "@aws-sdk/client-dynamodb": "^3.758.0",
    "@aws-sdk/lib-dynamodb": "^3.758.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "nanoid": "^5.1.3",
    "next": "15.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-grid-layout": "^1.5.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-resizable": "^3.0.5",
    "react-select": "^5.10.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.5",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-grid-layout": "^1.3.5",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "overrides": {
    "react-google-recaptcha-v3": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
